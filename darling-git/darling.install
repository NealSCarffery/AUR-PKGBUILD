post_install () {
  echo -e "\n\033[42m"; dyld ; echo -e "\033[0m\n"
  if [[ $(locale -a | grep french) == "french" ]];then echo -e "Attention, il manque des Frameworks. Les Frameworks disponibles sont dans le fichier \033[4m/etc/darling/dylib.conf\033[0m.\n"
  else
    echo -e "Warning, some Frameworks are missing. Frameworks are available in \033[4m/etc/darling/dylib.conf\033[0m file.\n"
  fi
}