pkgname=libreoffice-faenza-mod
pkgver=0.4.1
pkgrel=3
pkgdesc="An icons pack for LibreOffice, inspired by Faenza icons"
arch=('any')
url="http://gnome-look.org/content/show.php/Faenza+Icons++for+LibreOffice++4.0.0?content=157970"
license=('GPL')
depends=('libreoffice')
conflicts=('libreoffice-flat-icon-set')
install=$pkgname.install
source=("images_faenza_$pkgver.zip::https://dl.dropboxusercontent.com/u/37200621/images_faenza_$pkgver.zip")
noextract=("images_faenza_$pkgver.zip")
md5sums=('237d460fc081a12724316aff13633d8f')

package() {
	install -Dm0644 "images_faenza_$pkgver.zip" \
		"$pkgdir/usr/lib/libreoffice/share/config/images_faenza.zip"
}
